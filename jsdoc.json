{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 1,
    "source": {
        "include": ["client/script"],
        "includePattern": ".+\\.js$"
    },
    "sourceType": "script",
    "tags": {
        "allowUnknownTags": false,
        "dictionaries": ["jsdoc","closure"]
    },
    "opts": {
        "template": "templates/pixi",
        "encoding": "utf8",
        "destination": "doc/client",
        "recurse": true,
        "private": true
    },
    "templates": {
        "cleverLinks": true,
        "monospaceLinks": false,
        "default": {
            "outputSourceFiles": false,
            "includeDate": false,
            "useLongnameInNav": false
        },
        "applicationName": "Daphne"
    }
}